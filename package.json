    "next": "^15.0.3",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "typescript": "^5.9.3",
    "tailwindcss": "^3.4.18",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.3"
  }
